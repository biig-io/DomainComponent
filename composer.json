{
    "name": "biig/domain",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "symfony/event-dispatcher": "^4.3|^5.0",
        "doctrine/doctrine-bundle": "^1.8|^2.0",
        "doctrine/orm": "^2.6.3",
        "symfony/proxy-manager-bridge": "^4.4"
    },
    "authors": [
        {
            "name": "BiiG",
            "homepage": "https://www.biig.fr"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "doctrine/orm": "^2.6.3",
        "friendsofphp/php-cs-fixer": "^2.14",
        "symfony/symfony": "^4.3 || ^5.0"
    },
    "conflict": {
        "doctrine/orm": "<2.6.3"
    },
    "autoload": {
        "psr-4": {
            "Biig\\Component\\Domain\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Biig\\Component\\Domain\\Tests\\": "./Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
